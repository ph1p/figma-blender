{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "outDir": "figma-be"
  },
  "include": ["src"]
}
